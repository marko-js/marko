<!-- without keys -->
<for|val, i| of=arr>
  <div>
    ${i}
    : 
    ${val}
  </div>
  <div/>
  <div/>
</for>
<for|key, val| in=obj>
  <div>
    ${key}
    : 
    ${val}
  </div>
  <div/>
  <div/>
</for>
<for|i| from=0 to=10 step=2>
  <div>
    ${i}
  </div>
  <div/>
  <div/>
</for>
<!-- with keys -->
<for|val, i| of=arr>
  <div key=i>
    ${i}
    : 
    ${val}
  </div>
  <div/>
  <div key=`other-${i}`/>
</for>
<for|val, i, list| of=arr>
  <div key=i>
    ${list.length}
    : 
    ${val}
  </div>
</for>
<for|key, val| in=obj>
  <div key=key>
    ${key}
    : 
    ${val}
  </div>
  <div/>
  <div key=`other-${key}`/>
</for>
<for|i| from=0 to=10 step=2>
  <div key=i>
    ${i}
  </div>
  <div/>
  <div key=`other-${i}`/>
  <for|i| from=0 to=10 step=2>
    <div key=i>
      ${i}
    </div>
    <div/>
    <div key=`other-${i}`/>
  </for>
</for>
<for|i| from=10 to=0 step=-2>
  <div key=i>
    ${i}
  </div>
  <div/>
  <div key=`other-${i}`/>
</for>
