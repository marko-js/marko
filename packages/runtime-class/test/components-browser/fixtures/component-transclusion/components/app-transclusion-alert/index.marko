$ var type = state.type;
$ var className = "alert alert-" + type;
<div class=className role="alert">
    <div>
        <h1>ALERT! ${type}</h1>
        <div>
            <if(typeof state.body === "string")>${state.body}</if>
            <else>
                <${state.body}/>
            </else>
        </div>
    </div>
</div>