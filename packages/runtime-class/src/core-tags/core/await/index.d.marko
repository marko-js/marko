export type Input<T> = {
  value: readonly [T];
  then?: Marko.AttrTag<{ renderBody: Marko.Body<[Awaited<T>]> }>;
  catch?: Marko.AttrTag<{ renderBody: Marko.Body<[unknown]> }>;
  placeholder?: Marko.AttrTag<{ renderBody: Marko.Body }>;
  "client-reorder"?: boolean;
  name?: string;
  timeout?: number;
  "show-after"?: string;
} | {
  value: T;
  renderBody: Marko.Body<[Awaited<T>]>
}
