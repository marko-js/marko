<for to=5>
  <div/$el />
</for>

<script>
  const el = $el()
  if (el) {
    el.innerHTML = 'First Only';
  }
</script>

<hr />

<let/to=3 />
<for to=to>
  <div/$el2 />
</for>

<script>
  const el = $el2()
  if (el) {
    el.innerHTML = 'First Only';
  }
</script>

<hr />

<for|i| to=3>
  <ul>
  <for|j| to=3>
    <li/$el3 data-index=i*4+j />
  </for>
  </ul>
</for>

<script>
  let i = 0;
  for (const el of $el3) {
    el.innerHTML = `All (${i++})`;
  }
</script>