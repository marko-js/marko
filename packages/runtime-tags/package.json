{
  "name": "@marko/runtime-tags",
  "version": "6.0.69",
  "description": "Optimized runtime for Marko templates.",
  "keywords": [
    "api",
    "marko",
    "runtime",
    "tags"
  ],
  "homepage": "https://github.com/marko-js/marko",
  "bugs": "https://github.com/marko-js/marko/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marko-js/marko.git",
    "directory": "packages/runtime-tags"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./index.d.ts"
    },
    "./translator": "./src/translator/index.ts",
    "./tags/*": "./tags/*",
    "./debug/*": "./src/*.ts",
    "./*": "./src/*.ts"
  },
  "types": "index.d.ts",
  "files": [
    "dist",
    "tags",
    "index.d.ts",
    "tags-html.d.ts",
    "!**/meta.*.json",
    "!**/__tests__",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "node -r ~ts ./scripts/bundle.ts"
  },
  "dependencies": {
    "@marko/compiler": "^5.39.35",
    "csstype": "^3.1.3",
    "magic-string": "^0.30.17"
  },
  "engines": {
    "node": "18 || 20 || >=22"
  },
  "exports:override": {
    ".": {
      "types": "./index.d.ts"
    },
    "./package.json": "./package.json",
    "./translator": "./dist/translator/index.js",
    "./tags/*": "./tags/*",
    "./debug/*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/debug/*.mjs",
      "default": "./dist/debug/*.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "default": "./dist/*.js"
    }
  }
}
