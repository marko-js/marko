{
    "taglib-id": "marko-html",
    "tags": {
        "html": {
            "attributes": {
                "html-doctype": {
                    "type": "custom",
                    "preserve-name": true
                },
                "*": {
                    "type": "string",
                    "ignore": true
                }
            }
        },
        "html-doctype": {
            "attributes": {
                "value": {
                    "type": "custom"
                }
            },
            "node-class": "./DocTypeNode"
        },
        "*": {
            "transformer": "./html-tag-transformer"
        },
        "html-comment": {
            "renderer": "./CommentTag",
            "escape-xml-body": false
        }
    }
}