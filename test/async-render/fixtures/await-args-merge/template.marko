<ul>
    <li for(userId in [0, 1, 2, 3])>
        <await(name from data.userInfo) arg={ property:'name' } arg-userId=userId name=("user" + userId)>
            <ul>
                <li>
                    <b>Name:</b> ${name}
                </li>
            </ul>
        </await>
    </li>
</ul>
