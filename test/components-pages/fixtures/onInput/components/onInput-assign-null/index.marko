class {
    onInput(input) {
        this.input = null;
    }

    onMount() {
        var componentsLookup = window.components || (window.components = {});
        componentsLookup['onInput-assign-null'] = this;
    }
}

<div>
    Hello <span.name>${input ? input.name : '(no input)'}</span>!
</div>
