$ var tabs=input.tabs;

<div class="tabs">
    <ul>
        <li for(tab in tabs)>
            ${tab.title}
        </li>
    </ul>
    <div class="tab-content">
        <div class="tab" for(tab in tabs)>
            $ {
                if (tab.renderBody) {
                    tab.renderBody(out);
                }
            }
        </div>
    </div>
</div>