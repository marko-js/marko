            "deprecated": true,
            "set-context-flag": "hasLegacyWidgetAttr"
        },
        "@w-id": {
            "type": "string",
            "preserve-name": true,
            "deprecated": true,
            "set-context-flag": "hasLegacyWidgetAttr"
        },
        "@w-config": {
            "type": "expression",
