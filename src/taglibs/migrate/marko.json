            "type": "statement",
            "deprecated": true
        },
        "@unless": {
            "type": "statement",
            "deprecated": true
        },
        "@while": {
            "type": "statement",
            "deprecated": true
        },
        "@w-on*": {
            "pattern": true,
            "type": "string",
            "allow-expressions": true,
            "preserve-name": true,
            "set-flag": "hasComponentEvents",
