            "deprecated": true
        },
        "@w-preserve-body": {
            "type": "flag",
            "preserve-name": true,
            "deprecated": true
        },
        "@w-preserve-if": {
            "preserve-name": true,
            "deprecated": true
        },
        "@w-preserve-body-if": {
            "preserve-name": true,
